{
	"name": "rust-openssl-kdf",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"RH_CDN_ORGID": "${localEnv:RH_CDN_ORGID}",
			"RH_CDN_ACTIVATIONKEY": "${localEnv:RH_CDN_ACTIVATIONKEY}"
		}
	},
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"settings": {
		"files.watcherExclude": {
			"**/target/**": true
		},
		"rust-analyzer.checkOnSave.command": "clippy"
	},
	"extensions": [
		"mutantdino.resourcemonitor",
		"matklad.rust-analyzer",
		"tamasfe.even-better-toml",
		"serayuzgur.crates"
	],
	"remoteUser": "vscode"
}
